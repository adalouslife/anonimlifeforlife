{
  "tests": [
    {
      "name": "smoke-image",
      "input": {
        "image_url": "https://upload.wikimedia.org/wikipedia/commons/8/89/Portrait_Placeholder.png"
      },
      "assert": {
        "keys_present": ["status"],
        "status_equals": "completed"
      },
      "timeout": 120
    },
    {
      "name": "ping",
      "input": { "mode": "ping" },
      "assert": {
        "keys_present": ["status"],
        "status_equals": "completed"
      },
      "timeout": 60
    }
  ]
}
